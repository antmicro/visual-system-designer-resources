/ {
	compatible = "nxp,kw41z", "nxp,mkw41z4";
	chosen {
		zephyr,console = &lpuart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&gpioc{
	status = "okay";
};
&i2c0{
	status = "disabled";
};
&tpm1{
	status = "disabled";
};
&i2c1{
	status = "okay";
	pinctrl-0 = < &i2c1_default >;
	pinctrl-names = "default";
};
&lpuart0{
	status = "okay";
	pinctrl-0 = < &lpuart0_default >;
	pinctrl-names = "default";
	current-speed = < 0x1c200 >;
};
&ftfa{
	status = "okay";
};
&gpiob{
	status = "disabled";
};
&spi0{
	status = "okay";
	pinctrl-0 = < &spi0_default >;
	pinctrl-names = "default";
};
&tpm0{
	status = "okay";
	pinctrl-0 = < &tpm0_default >;
	pinctrl-names = "default";
};
&spi1{
	status = "disabled";
};
&tpm2{
	status = "okay";
	pinctrl-0 = < &tmp2_default >;
	pinctrl-names = "default";
};
&pinctrl{
	status = "okay";
};
&gpioa{
	status = "okay";
};
&adc0{
	status = "okay";
	pinctrl-0 = < &adc0_default >;
	pinctrl-names = "default";
};
