/ {
	compatible = "silabs,efm32wg_stk3800", "silabs,efm32wg";
	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&gpiod{
	status = "disabled";
};
&gpioa{
	status = "okay";
};
&gpiof{
	status = "okay";
};
&gpioc{
	status = "disabled";
};
&i2c0{
	status = "disabled";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	location-rx = < 0x1 0x4 0x1 >;
	location-tx = < 0x1 0x4 0x0 >;
};
&i2c1{
	status = "disabled";
};
&gpiob{
	status = "okay";
};
&gpioe{
	status = "okay";
};
&usart2{
	status = "disabled";
};
&usart1{
	status = "disabled";
};
&uart1{
	status = "disabled";
};
&leuart1{
	status = "disabled";
};
&usart0{
	status = "disabled";
};
&leuart0{
	status = "disabled";
};
&pinctrl{
	compatible = "silabs,gecko-pinctrl";
};