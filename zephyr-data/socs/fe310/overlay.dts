/ {
	compatible = "sifive,hifive1-revb";
	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &dtim;
	};
};
&spi1{
	status = "okay";
	pinctrl-0 = < &spi1_cs0_default &spi1_mosi_default &spi1_miso_default &spi1_sck_default >;
	pinctrl-names = "default";
};
&pwm1{
	status = "okay";
};
&uart1{
	status = "okay";
	current-speed = < 0x1c200 >;
};
&i2c0{
	status = "okay";
	clock-frequency = < 0x186a0 >;
	pinctrl-0 = < &i2c0_0_default &i2c0_1_default >;
	pinctrl-names = "default";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart0_rx_default &uart0_tx_default >;
	pinctrl-names = "default";
};
&pwm0{
	status = "okay";
};
&spi2{
	status = "okay";
	pinctrl-0 = < &spi1_cs2_default &spi1_mosi_default &spi1_miso_default &spi1_sck_default >;
	pinctrl-names = "default";
};
&spi0{
	status = "disabled";
};
&gpio0{
	status = "okay";
};
&pwm2{
	status = "okay";
};
