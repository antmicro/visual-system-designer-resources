/ {
	compatible = "nxp,lpc55xxx", "nxp,lpc";
	chosen {
		zephyr,console = &flexcomm0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&hs_lspi{
	status = "okay";
	pinctrl-0 = < &pinmux_hs_lspi_default >;
	pinctrl-names = "default";
	dmas = < &dma0 0x2 >, < &dma0 0x3 >;
	dma-names = "rx", "tx";
	cs-gpios = < &gpio1 0x1 0x1 >;
};
&gpio0{
	status = "okay";
};
&gpio1{
	status = "okay";
};
&usbfs{
	pinctrl-0 = < &pinmux_usbfs >;
	pinctrl-names = "default";
};
&sc_timer{
	status = "okay";
	pinctrl-0 = < &pinmux_sctimer_default >;
	pinctrl-names = "default";
};
&usbhs{
	status = "okay";
};
&flexcomm0{
	compatible = "nxp,lpc-usart";
	status = "okay";
	current-speed = < 0x1c200 >;
	dmas = < &dma0 0x4 >, < &dma0 0x5 >;
	dma-names = "rx", "tx";
	pinctrl-0 = < &pinmux_flexcomm0_usart >;
	pinctrl-names = "default";
};
&sram0{
	reg = < 0x20000000 0x30000 >;
};
&adc0{
	status = "okay";
	pinctrl-0 = < &pinmux_lpadc0 >;
	pinctrl-names = "default";
};
