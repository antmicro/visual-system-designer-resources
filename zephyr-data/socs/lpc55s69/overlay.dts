/ {
	compatible = "nxp,lpc55xxx", "nxp,lpc";
	chosen {
		zephyr,sram = &sram3;
		zephyr,flash = &flash0;
		zephyr,console = &flexcomm0;
	};
};
&iap{
	status = "disabled";
};
&usbfs{
	status = "disabled";
};
&usbhs{
	status = "disabled";
};
&gpio1{
	status = "okay";
};
&gpio0{
	status = "okay";
};
&hs_lspi{
	status = "disabled";
	pinctrl-0 = < &pinmux_hs_lspi_default >;
	pinctrl-names = "default";
};
&sc_timer{
	status = "disabled";
	pinctrl-0 = < &pinmux_sctimer_default >;
	pinctrl-names = "default";
};
&adc0{
	status = "disabled";
};
&flexcomm0{
	status = "okay";
	current-speed = < 0x1c200 >;
	dmas = < &dma0 0x4 >, < &dma0 0x5 >;
	dma-names = "rx", "tx";
	pinctrl-0 = < &pinmux_flexcomm0_usart >;
	pinctrl-names = "default";
};
