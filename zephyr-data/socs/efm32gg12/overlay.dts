/ {
	compatible = "silabs,efm32gg_sltb009a";
	chosen {
		zephyr,console = &usart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&usart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	location-rx = < 0x1 0x4 0x6 >;
	location-tx = < 0x1 0x4 0x7 >;
};
