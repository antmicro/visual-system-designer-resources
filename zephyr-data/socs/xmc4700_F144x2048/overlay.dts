/ {
	compatible = "infineon,xmc4700", "infineon,xmc4xxx";
	chosen {
		zephyr,console = &usic0ch0;
		zephyr,sram = &psram1;
		zephyr,flash = &flash0;
	};
};
&gpio15{
	status = "disabled";
};
&gpio2{
	status = "okay";
};
&gpio4{
	status = "disabled";
};
&gpio5{
	status = "okay";
};
&usic0ch0{
	status = "okay";
	compatible = "infineon,xmc4xxx-uart";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart_tx_p1_5_u0c0 &uart_rx_p1_4_u0c0 >;
	pinctrl-names = "default";
	input-src = "DX0B";
	interrupts = < 0x54 0x1 0x55 0x1 >;
	interrupt-names = "tx", "rx";
	fifo-start-offset = < 0x0 >;
	fifo-tx-size = < 0x10 >;
	fifo-rx-size = < 0x10 >;
};
&adc3{
	status = "disabled";
	vref-internal-mv = < 0xce4 >;
};
&gpio14{
	status = "okay";
};
&gpio3{
	status = "okay";
};
&adc1{
	status = "disabled";
	vref-internal-mv = < 0xce4 >;
};
&gpio0{
	status = "okay";
};
&adc2{
	status = "disabled";
	vref-internal-mv = < 0xce4 >;
};
&adc0{
	status = "disabled";
	vref-internal-mv = < 0xce4 >;
};
&gpio1{
	status = "okay";
};
&gpio6{
	status = "disabled";
};
