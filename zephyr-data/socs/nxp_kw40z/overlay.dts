/ {
	compatible = "nxp,kw40z", "nxp,mkw40z4";
	chosen {
		zephyr,console = &lpuart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&gpioa{
	status = "okay";
};
&i2c0{
	status = "disabled";
};
&ftfa{
	status = "disabled";
};
&lpuart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &lpuart0_default >;
	pinctrl-names = "default";
};
&i2c1{
	status = "disabled";
};
&spi0{
	status = "disabled";
};
&adc0{
	status = "okay";
	pinctrl-0 = < &adc0_default >;
	pinctrl-names = "default";
};
&spi1{
	status = "disabled";
};
&gpioc{
	status = "okay";
};
&gpiob{
	status = "disabled";
};
&pinctrl{
	status = "okay";
};
