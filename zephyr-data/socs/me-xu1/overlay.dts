/ {
	compatible = "xlnx,zynqmp-r5";
	chosen {
		zephyr,console = &uart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&uart1{
	status = "okay";
	current-speed = < 0x1c200 >;
	clock-frequency = < 0x5f5e09d >;
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	clock-frequency = < 0x5f5e09d >;
};
