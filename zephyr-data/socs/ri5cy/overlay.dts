/ {
	compatible = "openisa,rv32m1";
	chosen {
		zephyr,console = &lpuart0;
		zephyr,sram = &m4_dtcm;
		zephyr,flash = &m4_flash;
	};
};
&lpspi1{
	status = "okay";
	cs-gpios = < &gpiob 0x16 0x1 >;
	pinctrl-0 = < &lpspi1_default >;
	pinctrl-names = "default";
};
&lpuart1{
	pinctrl-0 = < &lpuart1_default >;
	pinctrl-names = "default";
};
&lpspi0{
	status = "okay";
	pinctrl-0 = < &lpspi0_default >;
	pinctrl-names = "default";
};
&lpuart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &lpuart0_default >;
	pinctrl-names = "default";
};
&lpi2c0{
	status = "okay";
	pinctrl-0 = < &lpi2c0_default >;
	pinctrl-names = "default";
};
&lpi2c3{
	status = "okay";
	pinctrl-0 = < &lpi2c3_default >;
	pinctrl-names = "default";
};
&tpm2{
	status = "okay";
	pinctrl-0 = < &tpm2_default >;
	pinctrl-names = "default";
};
&gpioa{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x19 >;
};
&gpioe{
	interrupt-parent = < &event1 >;
	interrupts = < 0x15 >;
};
&lpi2c1{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0xe >;
};
&lpi2c2{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0xf >;
};
&gpiod{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x1c >;
};
&tpm0{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x8 >;
};
&lpspi3{
	interrupt-parent = < &event1 >;
	interrupts = < 0x13 >;
};
&tpm1{
	interrupt-parent = < &intmux1_ch0 >;
};
&tpm3{
	interrupt-parent = < &event1 >;
	interrupts = < 0xf >;
};
&lpuart3{
	interrupt-parent = < &event1 >;
	interrupts = < 0x14 >;
};
&lpspi2{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x14 >;
};
&lpuart2{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x17 >;
};
&gpiob{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x1a >;
};
&gpioc{
	interrupt-parent = < &intmux1_ch0 >;
	interrupts = < 0x1b >;
};
