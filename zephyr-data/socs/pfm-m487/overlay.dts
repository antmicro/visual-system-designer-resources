/ {
	compatible = "nuvoton,pfm-m487", "nuvoton,m487";
	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&gpiod{
	status = "disabled";
};
&pinctrl{
	status = "okay";
};
&uart2{
	status = "disabled";
};
&uart3{
	status = "disabled";
};
&uart4{
	status = "disabled";
};
&uart7{
	status = "disabled";
};
&gpiog{
	status = "okay";
};
&uart5{
	status = "disabled";
};
&gpiob{
	status = "okay";
};
&gpioe{
	status = "disabled";
};
&flash0{
	reg = < 0x0 0x80000 >;
};
&uart1{
	status = "disabled";
};
&gpiof{
	status = "okay";
};
&gpioh{
	status = "okay";
};
&gpioa{
	status = "disabled";
};
&uart6{
	status = "disabled";
};
&gpioc{
	status = "disabled";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart0_default >;
	pinctrl-names = "default";
};
