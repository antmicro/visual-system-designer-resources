/ {
	compatible = "ti,am62x_m4_sk";
	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
	};
};
&uart0{
	status = "okay";
	pinctrl-0 = < &mcu_uart0_rx_default &mcu_uart0_tx_default >;
	pinctrl-names = "default";
};
&gpio0{
	status = "okay";
	pinctrl-0 = < &mcu_gpio0_led_default >;
	pinctrl-names = "default";
};
