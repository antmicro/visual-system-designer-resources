/ {
	compatible = "litex,vexriscv";
	chosen {
		zephyr,console = &uart0;
	};
};
&pwm0{
	status = "okay";
};
&eth0{
	status = "okay";
};
&gpio_in{
	status = "okay";
};
&spi0{
	status = "okay";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
};
&i2c0{
	status = "okay";
};
&gpio_out{
	status = "okay";
};
