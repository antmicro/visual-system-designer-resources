/ {
	compatible = "nxp,lpc55xxx", "nxp,lpc";
	chosen {
		zephyr,console = &flexcomm0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&iap{
	status = "okay";
};
&flexcomm0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &pinmux_flexcomm0_usart >;
	pinctrl-names = "default";
};
&hs_lspi{
	status = "okay";
	pinctrl-0 = < &pinmux_hs_lspi >;
	pinctrl-names = "default";
};
&usbhs{
	status = "okay";
};
&can0{
	status = "okay";
	bus-speed = < 0x1e848 >;
	bus-speed-data = < 0xf4240 >;
	pinctrl-0 = < &pinmux_can0 >;
	pinctrl-names = "default";
};
