/ {
	compatible = "arduino,due", "atmel,sam3x8e", "atmel,sam3x";
	chosen {
		zephyr,console = &uart;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&twi1{
	status = "okay";
	pinctrl-0 = < &twi1_default >;
	pinctrl-names = "default";
};
&eefc{
	status = "disabled";
};
&pwm0{
	status = "okay";
	pinctrl-0 = < &pwm0_default >;
	pinctrl-names = "default";
};
&usart3{
	status = "disabled";
};
&uart{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart_default >;
	pinctrl-names = "default";
};
&usart1{
	status = "disabled";
};
&twi0{
	status = "okay";
	pinctrl-0 = < &twi0_default >;
	pinctrl-names = "default";
};
&usart2{
	status = "disabled";
};
&usart0{
	status = "disabled";
};
