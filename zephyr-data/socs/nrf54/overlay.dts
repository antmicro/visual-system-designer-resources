/ {
	compatible = "nordic,nrf54h20pdk_nrf54h20-cpurad";
	chosen {
		zephyr,console = &uart135;
		zephyr,flash = &mram1x;
		zephyr,sram = &cpurad_ram0;
	};
};
&gpio0{
	status = "okay";
};
&gpio9{
	status = "okay";
};
&uart135{
	status = "okay";
	memory-regions = < &cpurad_dma_region >;
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart135_default >;
	pinctrl-1 = < &uart135_sleep >;
	pinctrl-names = "default", "sleep";
};
&uart136{
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart136_default >;
	pinctrl-1 = < &uart136_sleep >;
	pinctrl-names = "default", "sleep";
};
&pinctrl{
	compatible = "nordic,nrf-pinctrl";
	uart135_default: uart135_default {
		group1 {
			psels = < 0x24 >, < 0x20020 >;
		};
		group2 {
			bias-pull-up;
			psels = < 0x10025 >, < 0x30026 >;
		};
	};
	uart135_sleep: uart135_sleep {
		group1 {
			low-power-enable;
			psels = < 0x24 >, < 0x10025 >, < 0x20020 >, < 0x30026 >;
		};
	};
	uart136_default: uart136_default {
		group1 {
			psels = < 0x44 >, < 0x20040 >;
		};
		group2 {
			bias-pull-up;
			psels = < 0x10045 >, < 0x30046 >;
		};
	};
	uart136_sleep: uart136_sleep {
		group1 {
			low-power-enable;
			psels = < 0x44 >, < 0x10045 >, < 0x20040 >, < 0x30046 >;
		};
	};
};