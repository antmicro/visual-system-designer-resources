/ {
	compatible = "nxp,usb-kw24d512", "nxp,kw24d512", "nxp,kw2xd";
	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&gpiod{
	status = "okay";
};
&gpiob{
	status = "okay";
};
&gpioa{
	status = "okay";
};
&gpioe{
	status = "okay";
};
&adc0{
	status = "okay";
};
&usbd{
	num-bidir-endpoints = < 0x8 >;
	status = "okay";
};
&gpioc{
	status = "okay";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart0_default >;
	pinctrl-names = "default";
};
