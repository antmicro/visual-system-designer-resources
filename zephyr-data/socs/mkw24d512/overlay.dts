/ {
	compatible = "nxp,usb-kw24d512", "nxp,kw24d512", "nxp,kw2xd";
	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&pinctrl{
	status = "okay";
};
&uart1{
	status = "disabled";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart0_default >;
	pinctrl-names = "default";
};
&i2c1{
	status = "disabled";
};
&uart2{
	status = "disabled";
};
&adc0{
	status = "okay";
};
&gpioa{
	status = "okay";
};
&gpiod{
	status = "okay";
};
&gpioe{
	status = "okay";
};
&spi0{
	status = "disabled";
};
&gpiob{
	status = "okay";
};
&usbd{
	status = "okay";
};
&i2c0{
	status = "disabled";
};
&ftfl{
	status = "okay";
};
&gpioc{
	status = "okay";
};
