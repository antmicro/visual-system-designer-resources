/ {
	compatible = "sifive,FU540-C000", "fu540-dev", "sifive-dev";
	chosen {
		zephyr,console = &uart0;
	};
	ram0: ram0@80000000 {
		compatible = "memory";
		reg = <0x80000000 0xf0000000>;
		reg-names = "mem";
	};
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
};
&spi1{
	status = "okay";
};
&uart1{
	status = "disabled";
};
&gpio0{
	status = "okay";
};
&spi0{
	status = "disabled";
};
&spi2{
	status = "okay";
};
