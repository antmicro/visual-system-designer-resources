/ {
	compatible = "lairdconnect,bl5340-dvk-cpuapp";
	chosen {
		zephyr,console = &uart0;
		zephyr,flash = &flash1;
		zephyr,sram = &sram1;
	};
};
&gpio0{
	status = "okay";
};
&gpio1{
	status = "okay";
};
&i2c0{
	pinctrl-0 = < &i2c0_default >;
	pinctrl-1 = < &i2c0_sleep >;
	pinctrl-names = "default", "sleep";
};
&radio{
	fem = < &nrf_radio_fem >;
};
&spi0{
	cs-gpios = < &arduino_header 0x10 0x1 >;
	pinctrl-0 = < &spi0_default >;
	pinctrl-1 = < &spi0_sleep >;
	pinctrl-names = "default", "sleep";
};
&uart0{
	status = "okay";
	current-speed = < 0x1c200 >;
	pinctrl-0 = < &uart0_default >;
	pinctrl-1 = < &uart0_sleep >;
	pinctrl-names = "default", "sleep";
};
&pinctrl{
	compatible = "nordic,nrf-pinctrl";
	i2c1_default: i2c1_default {
		group1 {
			psels = < 0x180022 >, < 0x160023 >;
		};
	};
	i2c1_sleep: i2c1_sleep {
		group1 {
			psels = < 0x180022 >, < 0x160023 >;
			low-power-enable;
		};
	};
	spi2_default: spi2_default {
		group1 {
			psels = < 0xc001a >, < 0xa001b >, < 0x8001c >;
		};
	};
	spi2_sleep: spi2_sleep {
		group1 {
			psels = < 0xc001a >, < 0xa001b >, < 0x8001c >;
			low-power-enable;
		};
	};
	spi3_default: spi3_default {
		group1 {
			psels = < 0x8002e >, < 0xc002f >, < 0xa002d >;
		};
	};
	spi3_sleep: spi3_sleep {
		group1 {
			psels = < 0x8002e >, < 0xc002f >, < 0xa002d >;
			low-power-enable;
		};
	};
	spi4_default: spi4_default {
		group1 {
			psels = < 0xc000a >, < 0xa0009 >, < 0x80008 >;
		};
	};
	spi4_sleep: spi4_sleep {
		group1 {
			psels = < 0xc000a >, < 0xa0009 >, < 0x80008 >;
			low-power-enable;
		};
	};
	uart0_default: uart0_default {
		group1 {
			psels = < 0x14 >, < 0x20016 >, < 0x40013 >, < 0x60015 >;
		};
	};
	uart0_sleep: uart0_sleep {
		group1 {
			psels = < 0x14 >, < 0x20016 >, < 0x40013 >, < 0x60015 >;
			low-power-enable;
		};
	};
	uart1_default: uart1_default {
		group1 {
			psels = < 0x28 >, < 0x2002a >, < 0x40027 >, < 0x60029 >;
		};
	};
	uart1_sleep: uart1_sleep {
		group1 {
			psels = < 0x28 >, < 0x2002a >, < 0x40027 >, < 0x60029 >;
			low-power-enable;
		};
	};
	pwm0_default: pwm0_default {
		group1 {
			psels = < 0x2c001c >;
		};
	};
	pwm0_sleep: pwm0_sleep {
		group1 {
			psels = < 0x2c001c >;
			low-power-enable;
		};
	};
	qspi_default: qspi_default {
		group1 {
			psels = < 0x3a0011 >, < 0x3e000d >, < 0x40000e >, < 0x42000f >, < 0x440010 >, < 0x3c0012 >;
		};
	};
	qspi_sleep: qspi_sleep {
		group1 {
			psels = < 0x3a0011 >, < 0x3e000d >, < 0x40000e >, < 0x42000f >, < 0x440010 >, < 0x3c0012 >;
			low-power-enable;
		};
	};
};