/ {
	compatible = "andestech,adp_xc7k_ae350";
	chosen {
		zephyr,console = &uart1;
		zephyr,sram = &dram;
	};
};
&spi0{
	status = "okay";
};
&spi1{
	status = "okay";
	cs-gpios = < &gpio0 0x7 0x1 >;
};
&uart1{
	status = "okay";
	clock-frequency = < 0x12c0000 >;
	current-speed = < 0x1c200 >;
};
&i2c0{
	status = "okay";
};
&gpio0{
	status = "okay";
};
