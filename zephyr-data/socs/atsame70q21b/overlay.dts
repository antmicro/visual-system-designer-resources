/ {
	compatible = "tdk,robokit1", "atmel,same70q21", "atmel,same70";
	chosen {
		zephyr,console = &uart2;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};
&usart0{
	status = "disabled";
};
&uart4{
	status = "disabled";
};
&usart1{
	status = "disabled";
};
&usbhs{
	status = "okay";
};
&twihs0{
	status = "okay";
	pinctrl-0 = < &twihs0_default >;
	pinctrl-names = "default";
};
&afec1{
	status = "disabled";
};
&spi0{
	status = "okay";
	pinctrl-0 = < &spi0_default >;
	pinctrl-names = "default";
	dmas = < &xdmac 0x0 0x1 >, < &xdmac 0x1 0x2 >;
	dma-names = "tx", "rx";
	cs-gpios = < &pioa 0x1f 0x1 >, < &pioc 0x1f 0x1 >;
};
&can1{
	status = "disabled";
};
&uart2{
	status = "okay";
